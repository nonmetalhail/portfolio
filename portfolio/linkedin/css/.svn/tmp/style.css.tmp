  background:none;
  color:#777;
  text-shadow:none;
}
nav#primary a:active,
nav#primary a:focus {
  color:#777;
  background:none;
}

#photos {
-moz-column-count:    3;
-webkit-column-count: 3;
column-count:         3;
}

#photos img {
  opacity: 1;
}

} /*end max-width 768px*/

